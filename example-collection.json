{
  "info": {
    "name": "Example API Collection",
    "description": "Example Postman collection for oasify-postman testing",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get Users",
      "request": {
        "method": "GET",
        "url": "https://api.example.com/users"
      },
      "response": [
        {
          "name": "Success Response",
          "code": 200,
          "body": "{\n  \"users\": [\n    {\n      \"id\": 1,\n      \"name\": \"John Doe\",\n      \"email\": \"john@example.com\"\n    },\n    {\n      \"id\": 2,\n      \"name\": \"Jane Smith\",\n      \"email\": \"jane@example.com\"\n    }\n  ]\n}"
        }
      ]
    },
    {
      "name": "Get User by ID",
      "request": {
        "method": "GET",
        "url": "https://api.example.com/users/1"
      },
      "response": [
        {
          "name": "Success Response",
          "code": 200,
          "body": "{\n  \"id\": 1,\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\"\n}"
        }
      ]
    },
    {
      "name": "Create User",
      "request": {
        "method": "POST",
        "url": "https://api.example.com/users",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"New User\",\n  \"email\": \"newuser@example.com\"\n}"
        }
      },
      "response": [
        {
          "name": "Success Response",
          "code": 201,
          "body": "{\n  \"id\": 3,\n  \"name\": \"New User\",\n  \"email\": \"newuser@example.com\",\n  \"created_at\": \"2024-01-01T00:00:00Z\"\n}"
        }
      ]
    }
  ]
}
